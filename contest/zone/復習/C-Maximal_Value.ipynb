    "N = int(input())\n",
    "B = [int(a) for a in input().split()]\n",
    "A = []\n",
    "A.append(B[0])\n",
    "for b in range(N-2):\n",
    "    A.append(min(B[b], B[b+1]))\n",
    "A.append(B[-1])\n",
    "print(sum(A))"
